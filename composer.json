{
    "name": "gblix/laravel-controller-repository-traits",
    "description": "Traits para Controllers de Laravel facilitando uso da lib `prettus/l5-repository`",
    "type": "library",
    "authors": [
        {
            "name": "Stephan de Souza",
            "email": "stephan@gblix.net"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "lorisleiva/laravel-actions": "^2.8.4",
        "itsgoingd/clockwork": "^5.2",
        "graham-campbell/binput": "^11.1",
        "spatie/laravel-fractal": "^6.2.1",
        "prettus/l5-repository": "^2.9.1"
    },
    "require-dev": {
        "ekino/phpstan-banned-code": "^3.0.0",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0.1",
        "phpstan/phpstan-phpunit": "^2.0.3",
        "phpstan/phpstan-strict-rules": "^2.0.1",
        "squizlabs/php_codesniffer": "^3.5.5",
        "vimeo/psalm": "^5.26",
        "spatie/laravel-package-tools": "^1.16",
        "orchestra/testbench": "^9.8"
    },
    "autoload": {
        "psr-0": {
            "Gblix\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gblix\\Tests\\": "test/"
        }
    }
}
