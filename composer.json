{
    "name": "gblix/laravel-controller-repository-traits",
    "description": "Traits para Controllers de Laravel facilitando uso da lib `prettus/l5-repository`",
    "type": "library",
    "authors": [
        {
            "name": "Stephan de Souza",
            "email": "stephan@gblix.net"
        }
    ],
    "require": {
        "php": "^7.3 || ^8",
        "ext-json": "*",
        "prettus/l5-repository": "^2.7",
        "lorisleiva/laravel-actions": "^1.0||^2.0",
        "itsgoingd/clockwork": " ^5.0",
        "graham-campbell/binput": "^8.0||^9.0"
    },
    "require-dev": {
        "ekino/phpstan-banned-code": "^0.3.1",
        "phpstan/phpstan": "^0.12.55",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-phpunit": "^0.12.17",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "squizlabs/php_codesniffer": "^3.5.5",
        "orchestra/testbench": "^6.13",
        "vimeo/psalm": "^4.5.2"
    },
    "autoload": {
        "psr-0": {
            "Gblix\\": "src"
        }
    }
}
